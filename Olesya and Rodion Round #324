#include <iostream>     // std::cout
#include <algorithm>    // std::transform
#include <vector>       // std::vector
#include <functional>
#include <cstring>
#include <cmath>
#include <math.h>
#include <iomanip>
#include <string.h>
using namespace std;
 
int main()
{
    int n,t;
    cin>>n>>t;
    if(n==1 && t==10)
    {
        cout<<-1;
        return 0;
    }
    if(t!=10)
        {
            for(int i=1;i<=n;i++)
            {
                 cout<<t;
            }
        }
    else
    {
        for(int i=1;i<=n;i++)
            {
                 if(i==n)
                 {
                     cout<<0;
                 }
                 else
                 {
                     cout<<4;
                 }
            }
    }
 
    return 0;
}
