#include <iostream>     // std::cout
#include <algorithm>    // std::transform
#include <vector>       // std::vector
#include <functional>
#include <cstring>
#include <cmath>
#include <math.h>
#include <iomanip>
#include <string.h>
 
using namespace std;
 
int main()
{
     long long r,l;
     cin>>l>>r;
     long long a,b,c;
     if(l%2 != 0)
        l+=1;
     if(r-l < 2)
     {
         cout<<"-1";
         exit(0);
     }
     a = l; b = l+1; c = l+2;
     cout<<a<<" "<<b<<" "<<c;
    return 0;
}
