#include <iostream>     // std::cout
#include <algorithm>    // std::transform
#include <vector>       // std::vector
#include <functional>
#include <cstring>
#include <cmath>
#include <math.h>
#include <iomanip>
#include <string.h>
using namespace std;
 
int main()
{
    char arr[4][4];
    for(int i=0;i<4;i++)
    {
        for(int j=0;j<4;j++)
        {
            cin>>arr[i][j];
        }
 
    }
      for(int i=0;i<4;i++)
    {
        for(int j=0;j<3;j++)
        {
            if(arr[i][j]==arr[i][j+1])
            {
                if(arr[i+1][j]==arr[i][j] || arr[i+1][j+1]==arr[i][j+1])
                {
                    cout<<"YES";
                    return 0;
                }
                else if(arr[i-1][j]==arr[i][j] || arr[i-1][j+1]==arr[i][j+1])
                {
                    cout<<"YES";
                    return 0;
                }
 
            }
        }
    }
    cout<<"NO";
    return 0;
 
}
